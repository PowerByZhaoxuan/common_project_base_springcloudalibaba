        <logback.versin>1.4.8</logback.versin>

        <springdoc.version>1.7.0</springdoc.version>
        <springdoc-common.version>1.6.0</springdoc-common.version>
        <springdoc-webflux.version>2.1.0</springdoc-webflux.version>
        <sprongdoc-openapi.version>2.2.0</sprongdoc-openapi.version>

        <commons-fileupload.version>1.5</commons-fileupload.version>
        <snakeyaml.version>2.0</snakeyaml.version>
        <guava.version>32.1.1-jre</guava.version>

        <common.version>1.0.0-RELEASE</common.version>
        <remote_call.version>1.0.0-RELEASE</remote_call.version>
        <pojo.version>1.0.0-RELEASE</pojo.version>


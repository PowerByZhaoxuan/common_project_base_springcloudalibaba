        <spring-cloud-starter-alibaba-sentinel.version>2021.1</spring-cloud-starter-alibaba-sentinel.version>
        <spring-cloud-alibaba-sentinel-gateway.version>2021.1</spring-cloud-alibaba-sentinel-gateway.version>
        <sentinel-gateway-adapter.version>1.8.6</sentinel-gateway-adapter.version>
        <sentinel-transport-simple-http.version>1.8.6</sentinel-transport-simple-http.version>
        <spring-cloud-alibaba-sentinel-datasource.version>2022.0.0.0</spring-cloud-alibaba-sentinel-datasource.version>
        <sentinel-datasource-nacos.version>1.8.6</sentinel-datasource-nacos.version>
        <nacos-config-spring-boot-starter.version>0.2.12</nacos-config-spring-boot-starter.version>

